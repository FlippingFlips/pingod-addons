; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="BasicGameGodot"
config/description="Example game running Godot projects with Visual Pinball"
run/main_scene="res://MainScene.tscn"
config/features=PackedStringArray("4.0", "C#")
boot_splash/image="res://addons/assets/img/pingod-logo.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://addons/assets/img/pingod-logo.png"
config/windows_native_icon="res://addons/assets/img/pingod.ico"

[audio]

buses/default_bus_layout="res://assets/audio/audio_bus_layout.tres"
default_bus_layout="res://assets/audio/audio_bus_layout.tres"

[autoload]

WindowActions="*res://autoload/WindowActions.tscn"
AudioManager="*res://autoload/AudioManager.tscn"
PinGodGame="*res://autoload/PinGodGame.tscn"
Machine="*res://autoload/Machine.tscn"
MemoryMap="*res://autoload/MemoryMap.tscn"

[debug]

settings/fps/force_fps=31
debug/settings/stdout/print_fps=true

[display]

window/size/viewport_width=1024
window/size/viewport_height=600
window/size/borderless=true
window/size/always_on_top=true
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/size/test_width=1024.0
window/size/test_height=600.0
window/vsync/use_vsync=false

[dotnet]

project/assembly_name="BasicGameGodot"

[editor_plugins]

enabled=PackedStringArray("res://addons/pingod-window-commands/plugin.cfg", "res://addons/pingod-addons/plugin.cfg", "res://addons/pingod-audio/plugin.cfg", "res://addons/pingod-ballsave/plugin.cfg", "res://addons/pingod-game/plugin.cfg", "res://addons/pingod-machine/plugin.cfg", "res://addons/pingod-memorymap/plugin.cfg", "res://addons/pingod-plunger_lane/plugin.cfg", "res://addons/pingod-trough/plugin.cfg")

