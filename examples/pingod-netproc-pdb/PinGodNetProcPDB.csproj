    <None Include="autoload\Machine.tscn" />
    <None Include="autoload\MemoryMap.tscn" />
    <None Include="autoload\PinGodGame.tscn" />
    <None Include="autoload\Resources.tscn" />
    <None Include="autoload\WindowActions.tscn" />
    <None Include="modes\README.md" />
    <None Include="nuget.config" />
    <None Include="nuget\.gdignore" />
