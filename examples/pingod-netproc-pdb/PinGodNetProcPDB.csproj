<Project Sdk="Godot.NET.Sdk/4.0.0-beta.10">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <EnableDynamicLoading>true</EnableDynamicLoading>
    <Platforms>AnyCPU</Platforms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>   
	<GenerateTargetFrameworkAttribute>false</GenerateTargetFrameworkAttribute>   
	<IncludeSymbols>False</IncludeSymbols>
  </PropertyGroup>  
 
  <ItemGroup>
    <None Include="autoload\Adjustments.tscn" />
    <None Include="autoload\AudioManager.tscn" />
    <None Include="autoload\Machine.tscn" />
    <None Include="autoload\MemoryMap.tscn" />
    <None Include="autoload\PinGodGame.tscn" />
    <None Include="autoload\Resources.tscn" />
    <None Include="autoload\WindowActions.tscn" />
    <None Include="netproc.db" />
    <None Include="nuget.config" />
    <None Include="nuget\.gdignore" />
    <None Include="README.md" />
  </ItemGroup>  
 
  <ItemGroup>
    <PackageReference Include="NetProc.Data" Version="1.0.16.13" />
    <PackageReference Include="NetProc.Game" Version="1.0.16.13" />
  </ItemGroup>  
</Project>