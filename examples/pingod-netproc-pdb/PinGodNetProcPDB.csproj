  </ItemGroup>
 
  <ItemGroup>
    <Folder Include="scenes\AttractMode\" />
  </ItemGroup>
 
  <ItemGroup>
    <PackageReference Include="NetProc.Data" Version="1.0.27" />
    <!--<PackageReference Include="NetProc.Game" Version="1.0.25" />-->
  </ItemGroup>  
 

</Project>